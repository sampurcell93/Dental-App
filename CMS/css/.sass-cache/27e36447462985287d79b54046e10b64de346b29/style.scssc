@linei:@template"" @import "../bourbon/bourbon";

@mixin opacity($val: 1, $big: 100) { 

	opacity: #{$val};
	-moz-opacity: #{$val};
	filter:alpha(opacity=#{$big});

}

@mixin whiteGradient() { 

	background: #d9d9d9; /* Old browsers */
	background: -moz-linear-gradient(top,  rgba(255,255,255,1) 0%, rgba(229,229,229,1) 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(255,255,255,1)), color-stop(100%,rgba(229,229,229,1))); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(229,229,229,1) 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(229,229,229,1) 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(229,229,229,1) 100%); /* IE10+ */
	background: linear-gradient(to bottom,  rgba(255,255,255,1) 0%,rgba(229,229,229,1) 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#e5e5e5',GradientType=0 ); /* IE6-9 */



}

* {
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	margin: 0;
	padding: 0;
}

body {
	background: url(../images/blue.png);
	font-family: 'Source Sans Pro', sans-serif;
	color: #444;
	font-size: 20px;
	position: relative;
	@include transition(all 1s ease-in-out);

}

#lander_wrap {
	margin: 3% auto;
	width: 800px;
	@include border-radius(5px);
	@include box-shadow(inset 0px 1px 2px #fff, 0px 0px 3px #313131, 0px 0px 8px 0px #fff);
	background: rgba(255,255,255,.4);
	padding: 10px;
	position: relative;

 }

 #lander { 
 	width: 100%;
 	height: 100%;
	@include border-radius(5px);
	background: url(../images/brushedalum.png);
	position: relative;
	@include box-shadow(inset 0px 1px 2px #fff, 0px 0px 7px #999);
	padding-bottom: 20px;
	z-index: 0;

 }

h1 { 
	font-size: 210%;
	text-shadow: 0px 1px 0px #fff;
	padding: 3px 14px;
	@include border-radius( 3px 3px 0px 0px); 
	margin: 0px 0px 20px 0px;
	height: 50px;
	float: left;
	display: block;
	width: 100%;
}
h2 { 
	margin: 15px 10px 15px 0px;
	padding-left: 30px;
	display: block;
	width: 100%;
	font-family: 'Source Sans Pro', sans-serif;
	border-bottom: 1px dotted #313131;

}

h3 { padding: 3px 14px; margin: 15px 10px; }

h4 { 

	margin: 10px 6px;

}

p {  
	font-size: 80%;
	padding: 15px;
	text-shadow: 0px 1px 0px #fff;

}


textarea, input[type="text"] { 
	display: block;
	width: 80%;
	@include border-radius(4px);
	@include box-shadow(inset 0px 0px 10px #fff, 0px 0px 10px #aeaeae);
	padding: 7px;
	line-height: 23px;
	font-size: 75%;
	outline: none;
	margin: 20px;
	border: none;	
	max-width: 80%;
	height: 400px;
	float: left;

	&:focus { 

		@include box-shadow(0px 0px 6px 0px #313131);

	}
}

input[type="text"] { 

	float: none;
	width: 60%;
	height: 30px;
	margin: 20px auto 0;


}

[type="submit"] { 
	border: none;
	cursor: pointer; }

.button {
   border-top: 1px solid #f1d2fc;
   background: #472b47;
   background: -webkit-gradient(linear, left top, left bottom, from(#6e5770), to(#472b47));
   background: -webkit-linear-gradient(top, #6e5770, #472b47);
   background: -moz-linear-gradient(top, #6e5770, #472b47);
   background: -ms-linear-gradient(top, #6e5770, #472b47);
   background: -o-linear-gradient(top, #6e5770, #472b47);
   padding: 10px 20px;
   -webkit-border-radius: 8px;
   -moz-border-radius: 8px;
   border-radius: 8px;
   @include box-shadow(inset 0px 1px 1px  #fff);
   text-shadow: rgba(0,0,0,.4) 0 1px 0;
   color: #dedede;
   font-size: .75em;
   font-family: 'Source Sans Pro', sans-serif;
   text-decoration: none;
   vertical-align: middle;
   width: 300px;
   display: block;
   margin: 10px auto;
   text-align: center;
   cursor: pointer;

	&:active {
   border-top-color: #3b2040;
   background: #3b2040;
   }

   &:hover { color: #fff; }

  }

a { 

	color: purple;

	&:hover { color: #a44ace;}


}

hr { 

	margin: 20px auto 10px;
	width: 90%;

}

.content { position: relative; }

.console { 

	float: right;
	display: block;
	width: 100px;
	height: 400px;
	margin-right: 15px;
	margin-top: 20px;
	background: rgba(0,0,0,.7);
	@include border-radius(4px);

	li {

		cursor: pointer;
		background: #ebf1f6; /* Old browsers */
		background: -moz-linear-gradient(top,  #ebf1f6 0%, #abd3ee 50%, #89c3eb 51%, #d5ebfb 100%); /* FF3.6+ */
		background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ebf1f6), color-stop(50%,#abd3ee), color-stop(51%,#89c3eb), color-stop(100%,#d5ebfb)); /* Chrome,Safari4+ */
		background: -webkit-linear-gradient(top,  #ebf1f6 0%,#abd3ee 50%,#89c3eb 51%,#d5ebfb 100%); /* Chrome10+,Safari5.1+ */
		background: -o-linear-gradient(top,  #ebf1f6 0%,#abd3ee 50%,#89c3eb 51%,#d5ebfb 100%); /* Opera 11.10+ */
		background: -ms-linear-gradient(top,  #ebf1f6 0%,#abd3ee 50%,#89c3eb 51%,#d5ebfb 100%); /* IE10+ */
		background: linear-gradient(to bottom,  #ebf1f6 0%,#abd3ee 50%,#89c3eb 51%,#d5ebfb 100%); /* W3C */
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ebf1f6', endColorstr='#d5ebfb',GradientType=0 ); /* IE6-9 */
		@include box-shadow(inset 0px 0px 0px 1px #fff, 0px 0px 3px #aeaeae);
		border-top: 1px solid #fff;
		border-bottom: 1px solid #999;
		display: block;
		width: 80%;
		margin: 9px auto;
		@include border-radius(2px);
		height: 60px;
		text-align: center;
		line-height: 60px;
		font-size: 120%;
		position: relative;
		text-shadow: 0px 1px 0px #fff;

		&:hover { text-decoration: underline; }

	}

	.subList { 
		font-size: 100%;
		li {
			text-shadow: 0px 1px 0px #000;
		}

		&:hover #H4s { display: block;  }

	}
}

.help { 

	position: absolute;
	top: 7px;
	right: 17px;
	background: url(../images/gear.png) no-repeat 100% 0px ;
	cursor: pointer;	
	overflow: visible;
	padding-right: 30px;

}

aside {

	z-index: 100011;
	position: fixed;
	pointer-events: none;
	width: 500px;
	left: 50%;
	padding: 17px 17px 17px 20px;
	top: 100px;
	display: none;
	margin-left: -250px;
	@include box-shadow( 0px 0px 20px #000,inset 0px 0px 0px 1px #fff, 0px 0px 1003px 1000px #313131);
	border-top: 1px solid #fff;
	border-bottom: 1px solid #999;
	@include border-radius(2px);	
	@include whiteGradient();

	ul { 
		font-size: 80%; 
		padding-left: 20px;
	}

	.close { 
		height: 47px;
		top: 10px; }
}

.pointers { pointer-events: auto; }

#previewBox {
	position: absolute;
	left: 50%;
	top: 100px;
	margin-left: -350px;
	display: none;
	padding: 30px;
	width: 700px;
	@include border-radius(3px);
	@include whiteGradient();
	@include box-shadow(0px 0px 8px #313131);
	font-size: 80%; 

	&:after {

		position: absolute;
		top: -50px;
		left: 0;
		width: 680px;
		content: "Is this what you want to submit? You can still go back and change it.";
		display: block;
		@include border-radius(4px 4px 0px 0px);
		@include box-shadow(0px 0px 6px #313131);
		background: #472b47;
	    background: -webkit-gradient(linear, left top, left bottom, from(#6e5770), to(#472b47));
	    background: -webkit-linear-gradient(top, #6e5770, #472b47);
	    background: -moz-linear-gradient(top, #6e5770, #472b47);
	    background: -ms-linear-gradient(top, #6e5770, #472b47);
	    background: -o-linear-gradient(top, #6e5770, #472b47);
		margin-bottom: -13px;
		height: 30px;
		color: #e1e1e1;
		padding: 10px;
		text-align: center;
		z-index: 0;
		font-size: 1em;
	}
 }

.close  {

	position: absolute;
	top: -49px;
	right: 10px;
	width: 48px;
	height: 43px;
	background: url(../images/close.png) no-repeat;
	z-index: 11;
	cursor: pointer;

}

#H4s { 

	display: none;
	font-size: 70%;
	position: absolute;
	left: -95%;
	top: -35px;
	width: 100px;
	padding: 15px 0;
	z-index: 100;

	li { 
		width: 70px;
		margin: 0px auto;
		color: #e1e1e1;
		background: rgba(0,0,0,.7);
		height: 30px;
		line-height: 30px;
		@include box-shadow(0px 0px 4px #777);

		&:hover{ color: #fff; }
		 }

}

#appendix { 

	@extend .help;
	top: 34px;

}

#definition { 

	height: 200px;
	width: 100%;
	float: none;
	margin: 20px auto;

}

[id*="wrap"] { position: relative; }

.remove { 

		position: absolute;
		right: 10px;
		top: 9px;
		width: 140px;
		padding: 7px 8px;
}

#appendixAdd { top: 50px; }

.fixed { 

	position: fixed; 
	top: 0px;
	left: 50%;
	margin-left: -150px;
	z-index: 1000;
}
